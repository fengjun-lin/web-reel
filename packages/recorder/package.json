{
  "name": "@web-reel/recorder",
  "version": "1.0.0",
  "description": "Lightweight session recording SDK based on rrweb",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "rrweb": "^1.1.3"
  },
  "dependencies": {
    "idb": "^8.0.3",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "tsup": "^8.3.5",
    "typescript": "~5.9.3"
  },
  "keywords": [
    "rrweb",
    "session-recording",
    "replay",
    "debugging",
    "monitoring",
    "web-analytics"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/web-reel"
  }
}

